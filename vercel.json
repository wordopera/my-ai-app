{
    "buildCommand": "npm run build",
    "builds": [
      {
        "src": "app/api/**/*.ts",
        "use": "@vercel/node"
      },
      {
        "src": "scripts/**/*.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "app/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    # "env": {
    #   "PYTHON_VERSION": "3.12"
    # }
  }